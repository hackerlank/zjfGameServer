		<Byte notes="建筑类型(1 民居,2 农田 , 3兵营)" />
		<Array loopWith="Byte">
			<Byte notes="建筑ID" />
			<Byte notes="建筑等级" />
			<Int notes="当前每小时收益" />
			<Int notes="升级后的每小时收益" />
			<Int notes="升级所需费用" />
			<Int notes="升级所需时间，用秒表示" />
		</Array>
	</result>
</protocol>
